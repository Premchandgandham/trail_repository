<?xml version="1.0" ?>

<robot name="trail_robo_nav" >
    
  <material name="black">
    <color rgba="0.0 0.0 0.0 1.0"/>
  </material>
  <material name="blue">
    <color rgba="0.203125 0.23828125 0.28515625 1.0"/>
  </material>
  <material name="green">
    <color rgba="0.0 0.8 0.0 1.0"/>
  </material>
  <material name="grey">
    <color rgba="0.2 0.2 0.2 1.0"/>
  </material>
  <material name="orange">
    <color rgba="1.0 0.423529411765 0.0392156862745 1.0"/>
  </material>
  <material name="brown">
    <color rgba="0.870588235294 0.811764705882 0.764705882353 1.0"/>
  </material>
  <material name="red">
    <color rgba="0.80078125 0.12890625 0.1328125 1.0"/>
  </material>
  <material name="white">
    <color rgba="1.0 1.0 1.0 1.0"/>
  </material>  
  
  <gazebo reference="chassis">
    <material>Gazebo/Red</material>
  </gazebo>
  <gazebo reference="left_wheel">
    <material>Gazebo/Black</material>
  </gazebo>
  <gazebo reference="right_wheel">
    <material>Gazebo/Black</material>
  </gazebo>

  
<!-- fixed link  
<link name="robot_footprint"></link>
  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>
  
  -->
  
  <!--- trail_robo base link-->
  
    <link name="chassis">
    
    <pose>0 0 0.1 0 0 0</pose>
    
    <inertial>
      <mass value="2"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.028333" ixy="0" ixz="0" iyy="0.028333" iyz="0" izz="0.05333"/>
    </inertial>
    
    <collision name="collision_chassis">
      <geometry>
        <box size="0.4 0.4 0.1"/>
      </geometry>
    </collision>
    
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.4 0.4 0.1"/>
      </geometry>
      <material name="red"/>
    </visual>
    
    
    <inertial>
  	<mass value="0.25"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0004167" ixy="0" ixz="0" iyy="0.0004167" iyz="0" izz="0.0004167"/>
  </inertial>
     <collision name="caster_collision">
      <origin rpy=" 0 0 0" xyz="0.15 0 -0.05"/>
      <geometry>
        <sphere radius="0.049"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.5</slip1>
            <slip2>1.5</slip2>
          </ode>
        </friction>
      </surface>
    </collision>
    <visual name="caster_visual">
      <origin rpy=" 0 0 0" xyz="0.15 0 -0.05"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
      <material name="black"/>
    </visual>
    
    <inertial>
  	<mass value="0.25"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0004167" ixy="0" ixz="0" iyy="0.0004167" iyz="0" izz="0.0004167"/>
  </inertial>
     <collision name="caster_collision2">
      <origin rpy=" 0 0 0" xyz="-0.15 0 -0.05"/>
      <geometry>
        <sphere radius="0.049"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.5</slip1>
            <slip2>1.5</slip2>
          </ode>
        </friction>
      </surface>
    </collision>
    <visual name="caster_visual2">
      <origin rpy=" 0 0 0" xyz="-0.15 0 -0.05"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
      <material name="black"/>
    </visual>
     
    
    </link>
  
 <!-- 
  <link name="caster_wheel">
  
  <joint name="joint_caster_wheel" type="fixed">
    <origin rpy="0 0 0" xyz="0.19490 0 -0.05"/>   
    <parent link="chassis"/>
    <child link="caster_wheel" />
    <axis rpy="0 0 0" xyz="0 0 0"/>
  </joint>  
  
  -->
  
  
  <!-- Create wheel right -->  
    
  <link name="right_wheel">    
    <inertial>
      <mass value="0.2"/>
      <origin xyz="0 -0.025 0" rpy="1.5707 0 0"/>
      <inertia ixx="0.0005417" ixy="0" ixz="0" iyy="0.0005417" iyz="0" izz="0.001"/>
    </inertial>
    
    <collision name="right_wheel_collision">
       <origin xyz="0 -0.025 0" rpy="1.5707 0 0"/>
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>
    </collision>
    
    <visual name="right_wheel_visual">
       <origin xyz="0 -0.025 0" rpy="1.5707 0 0"/>
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>  
      <material name="black"/>    
    </visual>
    
  </link>
    
  <!--   Joint for right wheel  -->
  <joint name="joint_right_wheel" type="continuous">
    <origin rpy="0 0 0" xyz="0 -0.201 0"/>
    
    <parent link="chassis"/>
    <child link="right_wheel" />
    <axis xyz="0 1 0"/>

   </joint>
    
  <!-- Left Wheel link -->
    
  <link name="left_wheel">    
    <inertial>
      <mass value="0.2"/>
      <origin rpy="1.5707 0 0" xyz="0 0.025 0"/>
     <inertia ixx="0.0005417" ixy="0" ixz="0" iyy="0.0005417" iyz="0" izz="0.001"/>
    </inertial>
    
    <collision name="left_wheel_collision">
      <origin rpy="1.5707 0 0" xyz="0 0.025 0" />
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>
    </collision>
    
    <visual name="left_wheel_visual">
      <origin rpy="1.5707 0 0" xyz="0 0.025 0"/>
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>   
      <material name="black"/>    
    </visual>
    
  </link>
    
  <!--   Joint for left wheel  -->
  <joint name="joint_left_wheel" type="continuous">
    <origin rpy="0 0 0" xyz="0 0.201 0"/>
    <child link="left_wheel" />
    <parent link="chassis"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
   
  </joint>    
  
  
  <!--Camera-->
  
  <link name="camera">
  <inertial>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <mass value="0.1"/>
    <inertia ixx="1e-6" ixy="0.0" ixz="0.0" iyy="1e-6" iyz="0.0" izz="1e-6"/>
  </inertial>

  <visual name="camera_visual">
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <geometry>
      <box size="0.05 0.05 0.05"/>
    </geometry>
    <material name="white"/>
  </visual>
  <collision>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <geometry>
      <box size="0.05 0.05 0.05"/>
    </geometry>
    
  </collision>
</link>

<joint name="camera_head_joint" type="fixed">
  <origin xyz="0.175 0 0.0725" rpy="0.0 0 0"/>
  <parent link="chassis"/>
  <child link="camera"/>
  <axis xyz="0 0 0.0"/>
  <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
</joint>
  

<link name="sensor_hokuyo">
  <inertial>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <mass value="0.1"/>
    <inertia ixx="1e-6" ixy="0.0" ixz="0.0" iyy="1e-6" iyz="0.0" izz="1e-6"/>
  </inertial>

  <visual>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <geometry>
      <cylinder radius="0.02" length="0.04"/>
    </geometry>
    <material name="black"/>
  </visual>
  <collision>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <geometry>
      <cylinder radius="0.02" length="0.04"/>
    </geometry>
    
  </collision>
</link>

<joint name="sensor_head_joint" type="fixed">
  <origin xyz="0 0 0.045" rpy="0.0 0 0"/>
  <parent link="camera"/>
  <child link="sensor_hokuyo"/>
  <axis xyz="0 0 0.0"/>
</joint>
  
  
  <!-- Plugins -->
  
 <gazebo>
    <plugin name="differential_drive_controller" filename="libgazebo_ros_diff_drive.so">
      <legacyMode>false</legacyMode>
      <alwaysOn>true</alwaysOn>
      <updateRate>10</updateRate>
      <leftJoint>joint_left_wheel</leftJoint>
      <rightJoint>joint_right_wheel</rightJoint>
      <wheelSeparation>0.5</wheelSeparation>
      <wheelDiameter>0.2</wheelDiameter>
      <torque>100</torque>
      <commandTopic>cmd_vel</commandTopic>
      <odometryTopic>odom</odometryTopic>
      <odometryFrame>odom</odometryFrame>
      <robotBaseFrame>chassis</robotBaseFrame>
      <publishWheelTF>false</publishWheelTF>
      <publishWheelJointState>false</publishWheelJointState>
      <rosDebugLevel>na</rosDebugLevel>
      <wheelAcceleration>0</wheelAcceleration>
      <wheelTorque>50</wheelTorque>
      <odometrySource>world</odometrySource>
      <publishTf>1</publishTf>
      <publishOdomTF>true</publishOdomTF>
    </plugin>
  </gazebo>
  
  
  
  <gazebo reference="camera">
    <sensor type="camera" name="camera1">
      <update_rate>10.0</update_rate>
      <camera name="head">
        <horizontal_fov>1.3962634</horizontal_fov>
        <image>
          <width>800</width>
          <height>800</height>
          <format>R8G8B8</format>
        </image>
        <clip>
          <near>0.2</near>
          <far>300</far>
        </clip>
      </camera>
      <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
        <alwaysOn>true</alwaysOn>
        <updateRate>0.0</updateRate>
        <cameraName>camera</cameraName>
        <imageTopicName>rgb/image_raw</imageTopicName>
        <cameraInfoTopicName>rgb/camera_info</cameraInfoTopicName>
        <frameName>camera</frameName>
        <hackBaseline>0.07</hackBaseline>
        <distortionK1>0.0</distortionK1>
        <distortionK2>0.0</distortionK2>
        <distortionK3>0.0</distortionK3>
        <distortionT1>0.0</distortionT1>
        <distortionT2>0.0</distortionT2>
      </plugin>
    </sensor>
  </gazebo>
  
  <gazebo reference="sensor_hokuyo">
    <sensor type="ray" name="head_hokuyo_sensor">
      <pose>0 0 0 0 0 0</pose>
      <visualize>False</visualize>
      <update_rate>10</update_rate>
      <ray>
        <scan>
         <!-- <vertical >
            <samples>4</samples>
            <resolution>1</resolution>
            <min_angle>-0.0698132</min_angle>
            
            <max_angle>0</max_angle>
          </vertical> -->
          
          <horizontal>
            <samples>720</samples>
            <resolution>1</resolution>
            <min_angle>-1.570796</min_angle>
            <max_angle>1.570796</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.10</min>
          <max>30.0</max>
          <resolution>0.01</resolution>
        </range>
        <noise>
          <type>gaussian</type>
          <!-- Noise parameters based on published spec for Hokuyo laser
               achieving "+-30mm" accuracy at range < 10m.  A mean of 0.0m and
               stddev of 0.01m will put 99.7% of samples within 0.03m of the true
               reading. -->
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </ray>
      <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_laser.so">
        <topicName>hokuyo/scan</topicName>
        <frameName>sensor_hokuyo</frameName>
      </plugin>
    </sensor>
  </gazebo>
  
</robot>
